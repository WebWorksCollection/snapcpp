  <priority>-5</priority>
  <config>/etc/snapwebsites/snapfirewall.conf</config>
  <safe>firewall</safe>
  <recovery>60</recovery>
  <dependencies>
    <dependency>snapdbproxy</dependency>
  </dependencies>
</service>
