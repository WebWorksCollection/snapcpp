<snap-tree>
  <content path="/admin/settings/webform">
    <param name="content::title" revision="revision">Webform Settings</param>
    <param name="content::body" revision="revision">
      <![CDATA[<p>Setup how the Webform plugin is accessible and which fields are usable by other users.</p>]]>
    </param>
  </content>
  <content path="/admin/webform">
    <param name="content::title" revision="revision">Webform Editor</param>
    <param name="content::body" revision="revision">
      <![CDATA[<p>Click on the form you want to edit or on the New Form button.</p>]]>
    </param>
  </content>
  <content path="/images/webform">
    <param name="content::title" revision="revision">Webform List Images</param>
    <param name="content::body" revision="revision">
      <![CDATA[<p>Images used by the webform plugin.</p>]]>
    </param>
    <link name="permissions::direct::action::view" to="permissions::link_back::view" mode="*:*">/types/permissions/rights/view/attachment/public</link>
    <link name="permissions::direct::action::administer" to="permissions::link_back::administer" mode="*:*">/types/permissions/rights/administer/website/info</link>
    <attachment name="webform-logo" type="attachment/public">
      <path>:/images/webform/webform-logo-64x64.png</path>
      <mime-type>image/png</mime-type>
    </attachment>
  </content>
</snap-tree>
