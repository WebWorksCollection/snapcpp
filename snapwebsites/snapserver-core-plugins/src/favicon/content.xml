<snap-tree>
  <content path="/favicon.ico"> <!-- owner must stay favicon -->
    <!-- there is a problem and if no revision exists for this page,
         then the icon does not come up at all -->
    <param name="content::title" revision="revision">Favorite Icon Attachment</param>
    <param name="content::final" type="int8" revision="global">1</param>
    <link name="content::page_type" to="content::page" mode="1:*">/types/taxonomy/system/content-types/page/public</link>
    <link name="permissions::direct::action::view" to="permissions::link_back::view" mode="*:*">/types/permissions/rights/view/page/for-spammers</link>
    <link name="shorturl::no_shorturl" to="shorturl::no_shorturl" mode="1:*">/types/taxonomy/system/shorturl/no_shorturl</link>
  </content>
  <content path="/default-favicon.ico"> <!-- owner must stay favicon -->
    <param name="content::final" type="int8" revision="global">1</param>
    <link name="content::page_type" to="content::page" mode="1:*">/types/taxonomy/system/content-types/page/public</link>
    <link name="permissions::direct::action::view" to="permissions::link_back::view" mode="*:*">/types/permissions/rights/view/page/public</link>
    <link name="shorturl::no_shorturl" to="shorturl::no_shorturl" mode="1:*">/types/taxonomy/system/shorturl/no_shorturl</link>
  </content>
  <content path="/admin/settings/favicon"> <!-- owner must stay favicon so the default favicon can be used -->
    <param name="content::title" revision="revision">Favorite Icon Settings (favicon.ico)</param>
    <param name="content::body" revision="revision">
      <p>Site wide favorite icon settings.</p>
    </param>
    <param name="editor::layout" revision="global" overwrite="yes">"favicon-settings-page";</param>
    <param name="layout::layout" revision="global" overwrite="yes">"favicon-settings-parser";</param>
    <link name="permissions::direct::action::administer" to="permissions::link_back::administer" mode="*:*">/types/permissions/rights/administer/website/info</link>
    <link name="menu::admin" to="menu::admin" mode="1:*">/types/taxonomy/system/menu/item/admin</link>
  </content>
  <content path="/css/favicon" owner="output">
    <param name="content::title" revision="revision">Default CSS for Favicon Settings</param>
    <param name="content::body" revision="revision">
      <![CDATA[<p>The cascading style sheets for favicon forms generated by the editor.</p>]]>
    </param>
    <link name="permissions::direct::action::administer" to="permissions::link_back::administer" mode="*:*">/types/permissions/rights/administer/website/info</link>
    <attachment name="favicon" type="attachment/public">
      <path>qrc:/css/favicon/favicon.css</path>
      <mime-type>text/css</mime-type>
    </attachment>
  </content>
  <content path="/js/favicon" owner="output">
    <param name="content::title" revision="revision">Favicon Javascript</param>
    <param name="content::body" revision="revision">
      <![CDATA[<p>Various JavaScripts offered by the favicon plugin.</p>]]>
    </param>
    <link name="permissions::direct::action::administer" to="permissions::link_back::administer" mode="*:*">/types/permissions/rights/administer/website/info</link>
    <attachment name="favicon" type="attachment/public">
      <path>qrc:/js/favicon/favicon.js</path>
      <mime-type>text/javascript</mime-type>
    </attachment>
  </content>
  <content path="/images/favicon" owner="output">
    <param name="content::title" revision="revision">Favicon Images</param>
    <param name="content::body" revision="revision">
      <![CDATA[<p>Various images offered by the favicon plugin.</p>]]>
    </param>
    <link name="permissions::direct::action::administer" to="permissions::link_back::administer" mode="*:*">/types/permissions/rights/administer/website/info</link>
    <attachment name="asterisk" type="attachment/public">
      <path>qrc:/images/favicon/asterisk.ico</path>
      <mime-type>image/x-icon</mime-type>
    </attachment>
    <attachment name="checkmark" type="attachment/public">
      <path>qrc:/images/favicon/checkmark.ico</path>
      <mime-type>image/x-icon</mime-type>
    </attachment>
    <attachment name="earth" type="attachment/public">
      <path>qrc:/images/favicon/earth.ico</path>
      <mime-type>image/x-icon</mime-type>
    </attachment>
    <attachment name="five-colors" type="attachment/public">
      <path>qrc:/images/favicon/five-colors.ico</path>
      <mime-type>image/x-icon</mime-type>
    </attachment>
    <attachment name="ladybug" type="attachment/public">
      <path>qrc:/images/favicon/ladybug.ico</path>
      <mime-type>image/x-icon</mime-type>
    </attachment>
    <attachment name="molecule" type="attachment/public">
      <path>qrc:/images/favicon/molecule.ico</path>
      <mime-type>image/x-icon</mime-type>
    </attachment>
    <attachment name="ring" type="attachment/public">
      <path>qrc:/images/favicon/ring.ico</path>
      <mime-type>image/x-icon</mime-type>
    </attachment>
    <attachment name="snap" type="attachment/public">
      <path>qrc:/images/favicon/snap-favicon.ico</path>
      <mime-type>image/x-icon</mime-type>
    </attachment>
    <attachment name="star" type="attachment/public">
      <path>qrc:/images/favicon/star.ico</path>
      <mime-type>image/x-icon</mime-type>
    </attachment>
    <attachment name="yellow-ball" type="attachment/public">
      <path>qrc:/images/favicon/yellow-ball.ico</path>
      <mime-type>image/x-icon</mime-type>
    </attachment>
  </content>
</snap-tree>
