  <content path="/js/mimetype">
    <!-- MIME type JavaScripts generated using the magic library -->
    <param name="content::title" revision="revision">MIME Type JavaScripts</param>
    <param name="content::body" revision="revision">
      <![CDATA[<p>The different JavaScripts generated to handle the determination of a file type from its content.</p>]]>
    </param>
    <link name="permissions::direct::action::administer" to="permissions::link_back::administer" mode="*:*">/types/permissions/rights/administer/website/info</link>
    <attachment name="mimetype-basics" type="attachment/public">
      <path>:/js/mimetype/mimetype-basics.js</path>
      <mime-type>text/javascript</mime-type>
      <dependency>output (&gt;= 0.1.4)</dependency>
    </attachment>
    <!-- TODO: add standard and advanced once available -->
  </content>
