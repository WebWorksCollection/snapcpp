<snap-tree>
  <content path="/admin/settings/cookie-consent-silktide" owner="output">
    <param name="content::title" revision="revision">Cookie Consent silktide Settings</param>
    <param name="content::body" revision="revision">
      <![CDATA[<p>Settings of the cookie consent by <a href="https://silktide.com/tools/cookie-consent/" title="Visit silktide website">silktide</a>.</p>]]>
    </param>
    <param name="editor::layout" revision="global" overwrite="yes">"cookie-consent-silktide-page";</param>
    <param name="layout::layout" revision="global" overwrite="yes">"cookie-consent-silktide-parser";</param>
    <link name="permissions::direct::action::administer" to="permissions::link_back::administer" mode="*:*">/types/permissions/rights/administer/website</link>
  </content>
  <content path="/js/cookie-consent-silktide">
    <param name="content::title" revision="revision">Cookie Consent silktide JavaScripts</param>
    <param name="content::body" revision="revision">
      <![CDATA[<p>Cookie Consent silktide JavaScripts.</p>]]>
    </param>
    <link name="permissions::direct::action::administer" to="permissions::link_back::administer" mode="*:*">/types/permissions/rights/administer/website/info</link>
    <attachment name="cookie-consent-silktide.js" type="attachment/public">
      <path>:/js/cookie-consent-silktide/cookie-consent-silktide.js</path>
      <mime-type>text/javascript</mime-type>
      <dependency>jquery (&gt;= 1.10)</dependency>
    </attachment>
  </content>
  <content path="/images/cookie-consent-silktide">
    <param name="content::title" revision="revision">Cookie Consent silktide Images</param>
    <param name="content::body" revision="revision">
      <![CDATA[<p>Cookie Consent silktide Images.</p>]]>
    </param>
    <link name="permissions::direct::action::administer" to="permissions::link_back::administer" mode="*:*">/types/permissions/rights/administer/website/info</link>
    <attachment name="cookie-consent-silktide" type="attachment/public">
      <path>:/images/cookie-consent-silktide/cookie-consent-silktide-logo-64x64.png</path>
      <mime-type>image/png</mime-type>
    </attachment>
    <attachment name="dark-top" type="attachment/public">
      <path>:/images/cookie-consent-silktide/dark-top.png</path>
      <mime-type>image/png</mime-type>
    </attachment>
    <attachment name="dark-floating" type="attachment/public">
      <path>:/images/cookie-consent-silktide/dark-floating.png</path>
      <mime-type>image/png</mime-type>
    </attachment>
    <attachment name="dark-bottom" type="attachment/public">
      <path>:/images/cookie-consent-silktide/dark-bottom.png</path>
      <mime-type>image/png</mime-type>
    </attachment>
    <attachment name="light-top" type="attachment/public">
      <path>:/images/cookie-consent-silktide/light-top.png</path>
      <mime-type>image/png</mime-type>
    </attachment>
    <attachment name="light-floating" type="attachment/public">
      <path>:/images/cookie-consent-silktide/light-floating.png</path>
      <mime-type>image/png</mime-type>
    </attachment>
    <attachment name="light-bottom" type="attachment/public">
      <path>:/images/cookie-consent-silktide/light-bottom.png</path>
      <mime-type>image/png</mime-type>
    </attachment>
  </content>
</snap-tree>
