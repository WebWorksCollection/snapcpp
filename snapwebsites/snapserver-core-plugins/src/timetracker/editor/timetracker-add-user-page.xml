<editor-form owner="timetracker" id="settings" content-modified="2016-01-03 18:11:44">
  <mode action="edit"/>
  <immediate/>
  <focus refid="user"/>
  <on-save allow-edit="yes"/>
  <taborder>
    <tabindex refid="user"/>
  </taborder>
  <widget id="editor-session" type="hidden" field="editor::session" path="editor/session">
  </widget>

  <!-- user selector -->
  <widget id="user" type="line-edit" path="timetracker/user" no-toolbar="no-toolbar" auto-save="no">
    <tooltip>Enter the email address of one of your users.</tooltip>
    <help>Help</help>
  </widget>

</editor-form>
