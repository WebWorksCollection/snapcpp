<snap-tree>
  <content path="/js/locale-widgets">
    <!-- JavaScript code for the inline locale extension -->
    <param name="content::title" revision="revision">Locale Scripts</param>
    <param name="content::body" revision="revision">
      <![CDATA[<p>JavaScript code for the locale extension.</p>]]>
    </param>
    <link name="permissions::direct::action::administer" to="permissions::link_back::administer" mode="*:*">/types/permissions/rights/administer/website/info</link>
    <attachment name="locale-timezone" type="attachment/public">
      <path>qrc:/js/locale_widgets/locale-timezone.js</path>
      <mime-type>text/javascript</mime-type>
      <dependency>editor (&gt;= 0.0.3.245)</dependency>
    </attachment>
  </content>
  <content path="/css/locale-widgets">
    <param name="content::title" revision="revision">Locale CSS</param>
    <param name="content::body" revision="revision">
      <![CDATA[<p>CSS for the locale extension.</p>]]>
    </param>
    <link name="permissions::direct::action::administer" to="permissions::link_back::administer" mode="*:*">/types/permissions/rights/administer/website/info</link>
    <attachment name="locale-timezone" type="attachment/public">
      <path>qrc:/css/locale_widgets/locale-timezone.css</path>
      <mime-type>text/css</mime-type>
    </attachment>
  </content>
</snap-tree>
