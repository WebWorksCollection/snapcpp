<snap-tree>
  <content path="/js/form">
    <!-- JavaScript code for the enhanced Snap! forms -->
    <param name="content::title" revision="revision">Enhanced Snap! Forms</param>
    <param name="content::body" revision="revision">
      <![CDATA[<p>JavaScript code for the Enhanced Snap! Forms.</p>]]>
    </param>
    <link name="permissions::direct::action::administer" to="permissions::link_back::administer" mode="*:*">/types/permissions/rights/administer/website/info</link>
    <attachment name="form" type="attachment/public">
      <path>:/js/form/form.js</path>
      <mime-type>text/javascript</mime-type>
      <dependency>output (&gt;= 0.0.1)</dependency>
      <dependency>jquery-extensions (&gt;= 1.0.1)</dependency>
    </attachment>
  </content>
</snap-tree>
